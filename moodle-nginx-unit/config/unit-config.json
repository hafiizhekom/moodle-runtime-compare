{
    "listeners": {
        "*:80": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "action": {
                "pass": "applications/moodle"
            }
        }
    ],
    "applications": {
        "moodle": {
            "type": "php",
            "root": "/var/www/moodle",
            "index": "index.php",
            "user": "www-data",
            "group": "www-data",
            "options": {
                "file": "/etc/unit/php.ini"
            }
        }
    }
}